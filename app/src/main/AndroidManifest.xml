<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appviveresprimavera">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.AppViveresPrimavera">
        <activity android:name=".ActualizarEliminarProductoActivity"></activity>
        <activity android:name=".RegistroProductoActivity" />
        <activity android:name=".GestionProductosActivity" />
        <activity android:name=".DetalleProveedoresActivity" />
        <activity android:name=".ProveedoresActivitty" />
        <activity android:name=".DetalleProductoActivity" />
        <activity android:name=".ConsultarProductoActivity" />
        <activity android:name=".RegistroActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>